<div class="wh-start">
  <div class="wh-logo"></div>
  <h1>{{t 'start.title'}}</h1>

  <section>
    <center>
      {{#link-to 'wh.content.all-types' class="btn btn-neutral icon icon-leaf"}} {{t 'start.empty.action'}}{{/link-to}}
    </center>
    <hr>
    {{t 'start.empty.instruction'}}
  </section>
  <section>
    <center>
      {{#link-to 'theme' class="btn btn-neutral icon icon-rocket"}} {{t 'start.theme.action'}}{{/link-to}}
    </center>
    <hr>
    {{t 'start.theme.instruction'}}
  </section>
  <section>
    <center>
      {{#select-file action='importWordpressFile' accept='application/xml' class="icon icon-wordpress"}} {{t 'start.wordpress.action'}}{{/select-file}}
    </center>
    <hr>
    {{t 'start.wordpress.instruction'}}
  </section>
</div>
