{{#link-to 'wh.content.start' class="btn btn-neutral icon icon-plus-sign float-right"}} Add new content type{{/link-to}}

<h1>Your existing content types</h1>

<table class="wy-table wy-table-bordered-rows wh-content">
  <thead>
    <tr>
      <th>Content type</th>
      <th>Form style</th>
      <th>Items</th>
      <th>RSS</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    {{#each}}
      {{#link-to 'form' name tagName='tr'}}
        <td>{{name}}</td>
        <td>Time-based publishable</td>
        <td>22</td>
        <td><span class="icon icon-ok-sign"></span></td>
        <td class="wy-table-actions">
          <button class="btn btn-link icon icon-remove-sign" {{action deleteType this}}></button>
        </td>
      {{/link-to}}
    {{/each}}
  </tbody>
</table>
