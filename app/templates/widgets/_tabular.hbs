<table class="wy-table wy-table-bordered-all wy-table-striped wh-table-tabular">
  <thead>
    <tr>
      {{#each option in meta.options}}
        <th>{{option.value}}</th>
      {{/each}}
      <th></th>
    </tr>
  </thead>
  <tbody>
    {{#each row in value}}
      <tr>
        {{#each data in row}}
          <td>{{input value=data.value}}</td>
        {{/each}}
        <td><button type="button" class="btn btn-link icon icon-remove-sign" {{action "removeTabularRow" row this}}></button>
      </tr>
    {{/each}}
  </tbody>
</table>

<button type="button" class="btn btn-link btn-small icon icon-plus-sign" {{action "addTabularRow" this}}> {{t 'form.control.tabular.add'}}</button>
