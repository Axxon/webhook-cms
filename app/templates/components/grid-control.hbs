{{#if control.isInFormbuilder}}

  <div class="wh-grid-item first">
    <div class="wh-grid-value">
      <span class="icon icon-ellipsis-v"></span>
      <span class="wh-grid-num">0</span>
      <span class="wh-grid-value-name">First value</span>
    </div>
  </div>
  <div class="wh-grid-item active">
    <div class="wh-grid-value">
      <span class="icon icon-ellipsis-v"></span>
      <span class="wh-grid-num">1</span>
      <span class="wh-grid-value-name">First value</span>
    </div>
    <div class="wh-grid-form">
      {{view "formbuilder" model=control controller=formController}}
    </div>
  </div>
  <div class="wh-grid-item last">
    <div class="wh-grid-value">
      <span class="icon icon-ellipsis-v"></span>
      <span class="wh-grid-num">2</span>
      <span class="wh-grid-value-name">First value</span>
    </div>
  </div>

{{else}}

  {{view "grid-rows" gridControl=control}}

{{/if}}
